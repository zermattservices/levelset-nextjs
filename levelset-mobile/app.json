{
  "expo": {
    "name": "Levelset",
    "slug": "levelset-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "levelset",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#f2f5f4"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.levelset.app",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow Levelset to access your camera for scanning signatures and documents.",
        "NSFaceIDUsageDescription": "Allow Levelset to use Face ID for secure authentication.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#31664a"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.levelset.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow Levelset to use Face ID for secure authentication."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "edb34522-9d38-471b-9f08-b8b80f6c6891"
      }
    },
    "owner": "levelset"
  }
}
