{
  "crons": [
    {
      "path": "/api/cron/evaluate-certifications",
      "schedule": "0 8 * * *"
    },
    {
      "path": "/api/cron/sync-google-reviews",
      "schedule": "0 6 * * 0"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "has": [{ "type": "host", "value": "docs.levelset.io" }],
      "destination": "https://levelset.mintlify.dev/"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "docs.levelset.io" }],
      "destination": "https://levelset.mintlify.dev/:path*"
    }
  ]
}
