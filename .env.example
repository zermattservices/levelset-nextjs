# Supabase (required for all apps)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# MUI X Pro License (required for dashboard DataGrid/DatePicker)
NEXT_PUBLIC_MUI_X_LICENSE_KEY=your-license-key

# Agent service (apps/agent)
SUPABASE_URL=https://your-project.supabase.co
# SUPABASE_SERVICE_ROLE_KEY is shared with dashboard
PORT=3000

# Google Maps (required for location Google Places integration)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your-browser-api-key
GOOGLE_MAPS_API_KEY=your-server-api-key

# Outscraper (required for full Google review scraping)
OUTSCRAPER_API_KEY=your-outscraper-api-key

# Cron / Webhook secret (used for verifying cron jobs and Outscraper webhook callbacks)
CRON_SECRET=your-cron-secret

# Mobile (apps/mobile) â€” falls back to hardcoded values if not set
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
